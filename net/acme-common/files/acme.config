
config acme
	option account_email 'email@example.org'
	option debug '0'

config cert 'example'
	option enabled '0'
	list domains 'example.org'
	list domains '*.example.org'
	option validation_method 'dns'
	option dns 'dns_cf'
	list credentials 'CF_Token="xxxxxxxxxxxxxxxxxxxx"'
	list credentials 'CF_Account_ID="xxxxxxxxxxxxxxxxxxxx"'
	list credentials 'CF_Zone_ID="xxxxxxxxxxxxxxxxxxxx"'
	option key_type 'ec384'
	option days '60'

