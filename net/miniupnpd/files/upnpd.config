
config upnpd 'config'
	option enabled '0'
	option download_kbps '100000'
	option upload_kbps '50000'
	option port '5000'
	option upnp_lease_file '/var/run/miniupnpd.leases'
	option notify_interval '900'
	option clean_ruleset_threshold '20'
	option clean_ruleset_interval '600'
	option presentation_url 'http://192.168.9.1/'
	option serial_number '00000001'
	option model_number '1'
	option upnp_igd_compat 'igdv1'

config perm_rule
	option action 'allow'
	option ext_ports '1024-65535'
	option int_addr '0.0.0.0/0'
	option int_ports '1024-65535'
	option comment 'Allow high ports'

config perm_rule
	option action 'deny'
	option ext_ports '1-65535'
	option int_addr '0.0.0.0/0'
	option int_ports '1-65535'
	option comment 'Default deny'

